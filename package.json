    "url": "https://github.com/ilitteri/algo3-discord-bot/issues"
  },
  "homepage": "https://github.com/ilitteri/algo3-discord-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "child_process": "^1.0.2",
    "consola": "^2.15.3",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.8.0",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "node-cron": "^3.0.1",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
