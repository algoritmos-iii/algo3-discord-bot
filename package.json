    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/node-cron": "^3.0.1",
    "babel-jest": "^27.1.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "tsc-node": "^0.0.3"
  },
  "prettier": {
    "trailingComma": "es5",
